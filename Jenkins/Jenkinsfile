pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
              agent { docker { image 'maven:3.3.3' } }
              stages {
                  stage('build') {
                      steps {
                          sh 'mvn --version'
                      }
                  }
              }
            }
        }
        stage('Test') { 
            steps {
            echo 'Hello World!'
            }
        }
        stage('Deploy') { 
            steps {
            echo 'Hello World!'
            }
        }
    }
}