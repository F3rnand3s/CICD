pipeline {
    agent any
    stages {
        stage('Setup') {
          steps {
            echo 'Welcome to the Setup Stage!'
          }
        }

        stage('Build') {
          steps {
            echo 'Welcome to the Build Stage!'
          }
        }

        stage('Output') {
          steps {
            echo 'Welcome to the Output Stage!'
          }
        }
    }
}

