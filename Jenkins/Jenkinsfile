pipeline {
  agent any
    stages {
        stage('Setup') {
          steps {
            echo 'Welcome to the Setup Stage!'
            sh 'ls -la'
          }
        }

        stage('Build') {
          steps {
            echo 'Welcome to the Build Stage!'
          }
        }

        stage('Output') {
          steps {
            echo 'Welcome to the Output Stage!'
          }
        }

        stage('Cleanup') {
          steps {
            echo 'Welcome to the Cleanup Stage!'
            sh 'ls -la'
            sh 'rm -rf *'
            sh 'ls -la'
          }
        }
    }
}

