pipeline {
  agent none
    stages {
        stage('Setup') {
          steps {
            echo 'Welcome to the Setup Stage!'
            sh 'docker ps -a'
          }
        }

        stage('Build') {
          agent { docker { image 'node:latest' } }
          steps {
            echo 'Welcome to the Build Stage!'
          }
        }

        stage('Output') {
          steps {
            echo 'Welcome to the Output Stage!'
          }
        }
    }
}

